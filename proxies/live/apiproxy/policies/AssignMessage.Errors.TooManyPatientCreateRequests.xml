<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="AssignMessage.Errors.TooManyPatientCreateRequests">
    <AssignVariable>
        <Name>pds.error.code</Name>
        <Value>value</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>pds.error.coding.code</Name>
        <Value>TOO_MANY_REQUESTS</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>pds.error.coding.display</Name>
        <Value>Too many requests</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>pds.error.diagnostics</Name>
        <Template>There have been too many Create Patient requests. Please try again later.</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>