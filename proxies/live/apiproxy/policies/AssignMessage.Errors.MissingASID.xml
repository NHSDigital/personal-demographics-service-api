<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="AssignMessage.Errors.MissingASID">
    <AssignVariable>
        <Name>pds.error.code</Name>
        <Value>value</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>pds.error.coding.code</Name>
        <Value>MISSING_VALUE</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>pds.error.coding.display</Name>
        <Value>Required value is missing</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>pds.error.diagnostics</Name>
        <Template>No ASID is associated with your app. Please contact support.</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>
