<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.PostMethodNotAllowed">
  <FaultResponse>
    <Set>
      <Headers />
      <Payload contentType="text/plain" />
      <StatusCode>403</StatusCode>
      <ReasonPhrase>Forbidden</ReasonPhrase>
    </Set>
    <AssignVariable>
      <Name>pds.error.code</Name>
      <Value>forbidden</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>pds.error.coding.code</Name>
      <Value>INVALID_METHOD</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>pds.error.coding.display</Name>
      <Value>Cannot update resource without Healthcare Worker access token</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>pds.error.diagnostics</Name>
      <Template>Your app has insufficient permissions to use this method. Please contact support.</Template>
    </AssignVariable>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
